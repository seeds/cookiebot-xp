{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"baseUrl": "./src/main/resources",
		"moduleResolution": "node",
		"paths": {
			"/lib/xp/*": [
				"../../../build/libs/xp/*"
			],
			"/lib/extension/*": [
				"./lib/extension/*"
			],
			"/lib/external/*": [
				"./lib/external/*"
			],
			"/lib/jobs/*": [
				"./lib/jobs/*"
			],
			"/lib/listeners/*": [
				"./lib/listeners/*"
			],
			"/lib/modules/*": [
				"./lib/modules/*"
			],
			"/lib/util/*": [
				"./lib/util/*"
			],
			"/lib/*": [
				"../../../build/libs/*"
			],
			"/*": [
				"*"
			]
		}
	},
	"exclude": [
		"node_modules",
		"**/node_modules/*",
		"**/assets/*",
		"**/build/*",
		"**/bin/*",
	]
}
